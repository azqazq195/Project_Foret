package photo.dao;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import photo.bean.PhotoDTO;

@Repository
public class PhotoDAO {
	@Autowired
	SqlSessionTemplate sqlSession;
	
	public int photoWrite(PhotoDTO PhotoDTO) {
		return sqlSession.insert("mybatis.photoMapper.photoWrite", PhotoDTO);
	}
	
	public int photoDelete(PhotoDTO PhotoDTO) {
		return sqlSession.insert("mybatis.photoMapper.filesWrite", PhotoDTO);
	}
	
	public int photoModify(PhotoDTO PhotoDTO) {
		return sqlSession.insert("mybatis.photoMapper.filesWrite", PhotoDTO);
	}
}
