package region.controller;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import region.bean.RegionDTO;
import region.dao.RegionDAO;
import tag.bean.TagDTO;


@Service
public class RegionServiceImpl implements RegionService{
	
	@Autowired
	RegionDAO regionDAO;

	@Override
	public int regionWrite(RegionDTO regionDTO) {
		return regionDAO.regionWrite(regionDTO);
	}

	@Override
	public int regionDelete(RegionDTO regionDTO) {
		return regionDAO.regionDelete(regionDTO);
	}

	@Override
	public List<RegionDTO> regionList() {
		return regionDAO.regionList();
	}

	@Override
	public int memberRegionWrite(TagDTO tagDTO) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int memberRegionDelete(TagDTO tagDTO) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int foretRegionWrite(TagDTO tagDTO) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int foretRegionDelete(TagDTO tagDTO) {
		// TODO Auto-generated method stub
		return 0;
	}
}
