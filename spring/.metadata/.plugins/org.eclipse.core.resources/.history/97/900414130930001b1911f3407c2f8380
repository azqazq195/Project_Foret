<mapper namespace="mybatis.memberMapper">

<!-- 프리게시판 글 관리 -->
<insert id="memberWrite" parameterType="memberDTO" useGeneratedKeys="true" keyColumn="id" keyProperty="id">
	INSERT INTO member 
	     VALUES(seq_member_id.nextval,
	            #{name}, 
	            #{nickname}, 
	            #{birth}, 
	            #{email}, 
	            sysdate)
</insert>


