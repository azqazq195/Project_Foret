package photo.bean;

public class PhotoDTO {
	private int photo_id;		//사진번호(id)
	private String photo_name;	// 사진 파일 이름
	private String photo_path;	// 사진 파일 경로
	private String photo_type;	// 사진 종류
	private String photo_date;	// 사진 등록일
	
	public int getPhoto_id() {
		return photo_id;
	}
	public void setPhoto_id(int photo_id) {
		this.photo_id = photo_id;
	}
	public String getPhoto_name() {
		return photo_name;
	}
	public void setPhoto_name(String photo_name) {
		this.photo_name = photo_name;
	}
	public String getPhoto_path() {
		return photo_path;
	}
	public void setPhoto_path(String photo_path) {
		this.photo_path = photo_path;
	}
	public String getPhoto_type() {
		return photo_type;
	}
	public void setPhoto_type(String photo_type) {
		this.photo_type = photo_type;
	}
	public String getPhoto_date() {
		return photo_date;
	}
	public void setPhoto_date(String photo_date) {
		this.photo_date = photo_date;
	}

}
