<mapper namespace="mybatis.photoMapper">
	
		<insert id="filesWrite" parameterType="filesDTO">
		insert into files values
		(seq_files.nextval, #{member_seq}, #{dir}, #{filename}, #{originname}, #{filesize}, #{filetype}, sysdate)
	</insert>

</mapper>