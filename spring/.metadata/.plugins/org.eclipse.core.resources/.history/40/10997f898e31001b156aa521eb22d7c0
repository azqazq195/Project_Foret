package foret.dao;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import foret.bean.ForetDTO;
import member.bean.MemberDTO;

@Repository
public class ForetDAO {
	@Autowired
	SqlSessionTemplate sqlSession;
	public int foretWrite(ForetDTO foretDTO) {
		return sqlSession.insert("mybatis.foretMapper.foretWrite", memberDTO);
	}
	public int foretModify(ForetDTO memberDTO) {
		return sqlSession.update("mybatis.foretMapper.foretModify", memberDTO);
	}
	public int foretDelete(ForetDTO memberDTO) {
		return sqlSession.delete("mybatis.foretMapper.foretDelete", memberDTO);
	}
}
