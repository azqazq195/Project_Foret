package member.controller;

import member.bean.MemberDTO;

public interface MemberService {
	// 멤버 데이터
	public int memberWrite(MemberDTO memberDTO);
	public int memberModify(MemberDTO memberDTO);
	public int memberDelete(MemberDTO memberDTO);
	
	// 멤버 태그 링크
	public int memberTagWrite();
	public int memberTagModify();
	public int memberTagDelete();
	
	// 멤버 지역 링크
	public int memberRegionWrite();
	public int memberRegionModify();
	public int memberRegionDelete();
	
	// 멤버 게시글 좋아요 링크
	public int memberBoardLikeWrite();
	public int memberBoardLikeDelete();
	
	// 멤버 댓글 좋아요 링크
	public int memberCommentLikeWrite();
	public int memberCommentLikeDelete();
}
