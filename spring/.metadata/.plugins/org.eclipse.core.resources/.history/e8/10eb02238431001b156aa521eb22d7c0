package member.controller;

import member.bean.MemberDTO;
import member.bean.MemberLikeBoardDTO;

public interface MemberService {
	// 멤버 데이터
	public int memberWrite(MemberDTO memberDTO);
	public int memberModify(MemberDTO memberDTO);
	public int memberDelete(MemberDTO memberDTO);
	
	// 멤버 좋아요
	public int memberBoardLike(MemberLikeBoardDTO memberLikeBoardDTO);
	public int memberBoardDisLike(MemberLikeBoardDTO memberLikeBoardDTO);
	public int memberCommentLike(MemberLikeBoardDTO memberLikeBoardDTO);
	public int memberBoardLike(MemberLikeBoardDTO memberLikeBoardDTO);
}
