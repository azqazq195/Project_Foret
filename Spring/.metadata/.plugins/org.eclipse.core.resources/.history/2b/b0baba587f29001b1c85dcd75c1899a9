<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.member">

<!-- 프리게시판 글 관리 -->
<insert id="writeMember" parameterType="Member">
insert into member_table values (seq_member_id.nextval, #{member_name}, #{member_nickname}, #{member_email}, #{member_birth}, sysdate, #{member_chain}, #{member_address}, #{member_photo});
</insert>

<update id="modifyMember" parameterType="Member">
update freeboard_table set freeboard_subject=#{freeboard_subject}, freeboard_content=#{freeboard_content}, freeboard_edit_date=sysdate 
where freeboard_no=#{freeboard_no} and member_id=#{member_id}
</update>

</mapper>